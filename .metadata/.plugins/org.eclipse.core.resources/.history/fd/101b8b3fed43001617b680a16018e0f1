$(function() {
	// 切换导航状态
	toggleNav(6, 3, 0);
});
function selectAll(){
	if($("#allCheckbox").is(":checked")){
		$(".ckClassFlg").prop('checked',true);
	}else{
		$(".ckClassFlg").prop('checked',false);
	}
}
function showAddPackingModal() {
	$('#addPackingForm').form('clear');
	$('#addPackingModal').modal('open');
}